.memberRightsEditView {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  z-index: 888;
}

#classViewCover {
  width: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  background-color: #333;
  opacity: 0.7;
}

 .memberRightsBgView {
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 10%;
  top: calc(50% - 160px);
  width: 80%;
  height: 260px;
  background-color: white;
  border-radius: 8px;
} 

.memberBgTitleView{
  display: flex;
  align-items: center;
  width: 100%;
  height: 60px;
}

.memberBgTitle{
  width: 100%;
  height: 20px;
  line-height: 20px;
  text-align: center;
  font-size: 16px;
}

.memberCancleButton{
  position: absolute;
  font-size: 15px;
  right: 0px;
  width: 60px;
  height: 20px;
  line-height: 20px;
  color: #4D9FD8;
  text-align: center;
}

.memberRightsBgLine{
  margin: 0px;
  width: 100%;
  height: 1px;
  background-color: #E6E6E6;
}

.offsetAmountView{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0px;
  width: 100%;
  height: 50px;
}

.offsetAmountTitle{
  margin-left: 10px;
  width: 100px;
  height: 25px;
  line-height: 25px;
  font-size: 14px;
}

.offsetAmountValue{
  margin-right: 10px;
  width: 150px;
  height: 25px;
  line-height: 25px;
  font-size: 14px;
  text-align: right;
}

.offsetAmountInput{
  margin-top: 15px;
  width: 90%;
  height: 30px;
  font-size: 14px;
  border: 1px solid #E6E6E6;
  border-radius: 3px;
}

.memberRightsBgBottomLine{
  margin-top: 30px;
  width: 100%;
  height: 1px;
  background-color: #E6E6E6;
}

.memberRightsSureButton{
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 20px;
  width: 90%;
  height: 36px;
  line-height: 36px;
  color: white;
  background-color: #4D9FD8;
  font-size: 14px;
  border-radius: 5px;
}

.show {
  display: block;
}

.hide {
  display: none;
}
