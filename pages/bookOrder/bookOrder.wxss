/* pages/bookOrder/bookOrder.wxss */

page {
  background-color: #fbf9fe;
}

.scrollView {
  margin: 0px;
  width: 100%;
  padding-bottom: 55px;
}

.deliveryModeView {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 5px;
  background-color: white;
  width: 100%;
  height: 60px;
}

.deliveryModeTitle {
  margin-left: 15px;
  width: 50%;
  font-size: 18px;
  color: #222;
  line-height: 80px;
}

.deliveryModeImage {
  margin-right: 20px;
  width: 94px;
  height: 35px;
}

.deliveryModeValue {
  margin-right: 20px;
  font-size: 17px;
}

.setDefaultAddressView{
  display: flex;
  margin-top: 5px;
  width: 100%;
  height: 50px;
  align-items: center;
  background-color: white;
}

.setDefaultAddressText{
  margin-left: 15px;
  font-size: 15px;
  width: calc(100% - 40px);
}

.addressInfoView {
  display: flex;
  align-items: center;
  margin-top: 5px;
  width: 100%;
  height: 90px;
  background-color: white;
}

.addressIconImage {
  margin-left: 10px;
  width: 23px;
  height: 32px;
}

.contactInfoView {
  display: flex;
  flex-direction: column;
  margin-left: 5px;
  width: 80%;
}

.contactNameView {
  display: flex;
  align-items: center;
  width: 100%;
  height: 30px;
}

.contactAddressView {
  display: flex;
  align-items: center;
  width: 100%;
  height: 40px;
}

.rightIconImage {
  width: 20px;
  height: 20px;
}

.productContentView {
  display: flex;
  flex-flow: column;
  margin-top: 5px;
  width: 100%;
}

.productView {
  display: flex;
  margin-top: 1px;
  width: 100%;
  height: 100px;
  align-items: center;
  background-color: white;
}

.productImage {
  margin-left: 10px;
  width: 80px;
  height: 80px;
}

.productInfo {
  display: flex;
  flex-direction: column;
  margin-left: 10px;
  width: calc(100% - 100px);
}

.productNameText {
  font-size: 15px;
  width: 100%;
  height: 25px;
  line-height: 25px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.priceView {
  margin-right: 10px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 30px;
}

.numText {
  font-size: 14px;
  text-align: right;
  /* font-weight: 300; */
}

.price {
  font-size: 14px;
  /* font-weight: 300; */
}

.specificationsText {
  font-size: 13px;
  color: #999999;
}

.shouldPayAmountView {
  display: flex;
  align-items: center;
  margin-top: 5px;
  width: 100%;
  height: 60px;
  background-color: white;
}

.shouldPayAmountText {
  margin-right: 10px;
  width: calc(50% - 10px);
  text-align: right;
  font-size: 18px;
  font-weight: 800;
}

.shouldPayAmountTitle {
  margin-left: 5px;
  width: 50%;
  font-size: 17px;
  color: #222;
}

.memberRightsView {
  display: flex;
  margin-top: 5px;
  width: 100%;
  background-color: white;
}

.shouldPayIconImage {
  margin-left: 10px;
  width: 21px;
  height: 20px;
}

.memberRightsIconImage {
  margin-left: 10px;
  margin-top: 18px;
  width: 20px;
  height: 16px;
}

.memberContentView {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 5px;
  width: calc(100% - 35px);
}

.memberRightsTitleView {
  width: 100%;
  height: 50px;
  line-height: 50px;
  font-size: 17px;
}

.memberLineImage {
  width: 100%;
  height: 1px;
}

.memberDiscountView {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.memberDiscounTextView {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 38px;
}

.memberDiscounTextTitle {
  font-size: 14px;
  /* font-weight: 300; */
}

.memberDiscounTextValue {
  margin-right: 10px;
  font-size: 14px;
  text-align: right;
  color: #999;
}

.storeInfoView {
  display: flex;
  margin-top: 5px;
  width: 100%;
  background-color: white;
  padding-bottom: 5px;
}

.storeIconImage {
  margin-left: 10px;
  margin-top: 10px;
  width: 23px;
  height: 32px;
}

.storeContentView {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 5px;
  width: calc(100% - 35px);
}

.storeTitleView {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 50px;
  line-height: 50px;
  font-size: 16px;
}

.storeInfoTitle {
  width: 50%;
  font-size: 17px;
  color: #222;
  line-height: 80px;
}

.storeNumText {
  margin-right: 10px;
  font-size: 14px;
  text-align: center;
}

.storeLineImage {
  width: 100%;
  height: 1px;
}

.storeAddressInfoView {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
}

.storeContactInfoView {
  display: flex;
  flex-direction: column;
  width: 70%;
}

.storeContacterLine {
  margin-left: 10px;
  width: 1px;
  height: 92rpx;
}

.callPhoneView {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 1px;
  width: calc(30% - 12px);
}

.callImage {
  width: 32px;
  height: 32px;
}

.storeNameView {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 5px;
  width: 100%;
}

.storeAddressView {
  margin-top: 5px;
  width: 100%;
  font-size: 13px;
  /* font-weight: 300; */
}

.storeNameTextView {
  display: flex;
  align-items: center;
  margin-top: 0px;
  width: 70%;
  height: 100%;
  font-size: 15px;
}

.storeDistanceView {
  align-items: center;
  width: 30%;
  font-size: 12px;
  text-align: right;
}

.footView {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  width: 100%;
  height: 50px;
  bottom: 0px;
  background-color: white;
}

.offerOrderView {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 90%;
  height: 36px;
  background-color: #4d9fd8;
  border-radius: 3px;
}

.offerOrderText {
  color: white;
  font-size: 15px;
}

.memberRightsEditView {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  z-index: 888;
}

#classViewCover {
  width: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  background-color: #333;
  opacity: 0.7;
}

.memberRightsBgView {
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 10%;
  top: calc(50% - 160px);
  width: 80%;
  height: 260px;
  background-color: white;
  border-radius: 8px;
}

.memberBgTitleView {
  display: flex;
  align-items: center;
  width: 100%;
  height: 60px;
}

.memberBgTitle {
  width: 100%;
  height: 20px;
  line-height: 20px;
  text-align: center;
  font-size: 16px;
}

.memberCancleButton {
  position: absolute;
  font-size: 15px;
  right: 0px;
  width: 60px;
  height: 20px;
  line-height: 20px;
  color: #4d9fd8;
  text-align: center;
}

.memberRightsBgLine {
  margin: 0px;
  width: 100%;
  height: 1px;
  background-color: #e6e6e6;
}

.offsetAmountView {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0px;
  width: 100%;
  height: 50px;
}

.offsetAmountTitle {
  margin-left: 10px;
  width: 100px;
  height: 25px;
  line-height: 25px;
  font-size: 14px;
}

.offsetAmountValue {
  margin-right: 10px;
  width: 150px;
  height: 25px;
  line-height: 25px;
  font-size: 14px;
  text-align: right;
}

.offsetAmountInput {
  margin-top: 15px;
  width: 90%;
  height: 30px;
  font-size: 14px;
  border: 1px solid #e6e6e6;
  border-radius: 3px;
}

.memberRightsBgBottomLine {
  margin-top: 30px;
  width: 100%;
  height: 1px;
  background-color: #e6e6e6;
}

.memberRightsSureButton {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 20px;
  width: 90%;
  height: 36px;
  line-height: 36px;
  color: white;
  background-color: #4d9fd8;
  font-size: 14px;
  border-radius: 5px;
}

.show {
  display: block;
}

.hide {
  display: none;
}
