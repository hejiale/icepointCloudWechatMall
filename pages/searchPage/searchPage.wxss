/* pages/searchPage/searchPage.wxss */

page {
  background-color: #f5f5f5;
}

.alertView {
  display: flex;
  flex-direction: column;
  margin-top: 200px;
  width: 100%;
}

.textView {
  display: flex;
  align-content: center;
  height: 50px;
  width: 100%;
}

.clickView {
  display: flex;
  align-items: center;
  bottom: 10px;
  width: 100%;
  height: 50px;
}

.searchContentView {
  display: flex;
  flex-direction: column;
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  z-index: 888;
}

.inputSearch {
  margin-left: 5px;
  height: 30px;
  font-size: 13px;
  width: calc(100% - 25px);
  background-color: white;
}

.hotSearchView {
  position: absolute;
  display: flex;
  flex-direction: column;
  margin-top: 50px;
  width: 100%;
  height: calc(100% - 50px);
}

.hotSearchTopView {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-left: 10px;
  height: 30px;
}

.hotTitle {
  width: 100px;
  line-height: 20px;
  font-size: 13px;
}

.hotItemView {
  margin-top: 10px;
  width: 100%;
}

.search-icon {
  margin-left: 5px;
  width: 30rpx;
  height: 30rpx;
}

.searchClassItem {
  display: inline-block;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-left: 10px;
  margin-top: 5px;
  padding-left: 10px;
  padding-right: 10px;
  height: 30px;
  text-align: center;
  border-radius: 5px;
}

.searchClassItemText {
  display: inline-block;
  line-height: 30px;
  font-size: 13px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.searchRightView {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50px;
  height: 100%;
}

.contentView {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.topMenuView {
  position: fixed;
  display: flex;
  flex-direction: row;
  align-items: center;
  top: 0px;
  width: 100%;
  height: 45px;
}

.classImageView {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 40px;
  height: 100%;
}

.scanText {
  margin-top: 5px;
  width: 100%;
  height: 18px;
  font-size: 10px;
  text-align: center;
  color: grey;
}

.search-input {
  display: flex;
  flex-direction: row;
  width: calc(100% - 60px);
  height: 30px;
  margin-left: 10px;
  border-radius: 5px;
  align-items: center;
  background-color: white;
}

.filter-icon {
  width: 18px;
  height: 16px;
}

.filterTypeText {
  line-height: 30px;
  font-size: 12px;
  color: lightgrey;
}

.search-icon-content {
  position: fixed;
  top: 15rpx;
  right: 45px;
  width: 30rpx;
  height: 30rpx;
}

.searchSpaceLine {
  margin-left: 5px;
  width: 1px;
  height: 10px;
  background-color: lightgray;
}

.serach-text {
  margin: 5px;
  line-height: 30px;
  font-size: 12px;
}

.class-img {
  width: 16px;
  height: 16px;
}

.storeText {
  font-size: 12px;
  color: gray;
}

.productListView {
  position: absolute;
  top: 45px;
  width: calc(100% - 5px);
  height: calc(100% - 45px);
}

.productContentView {
  display: inline-block;
  flex-direction: column;
  align-items: center;
  padding-left: 5px;
  width: 50%;
  height: 190px;
  box-sizing: border-box;
}

.productView {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: white;
  height: calc(100% - 5px);
}

.productImage {
  width: 80%;
  height: 112px;
}

.productInfoView {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 0px;
  width: 100%;
  height: 120rpx;
}

.productText {
  width: 80%;
  font-size: 12px;
  color: gray;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}

.priceText {
  margin-top: 10px;
  font-size: 14px;
  font-family: "Helvetica Neue",
                 Helvetica,
                 Arial,
                 "Hiragino Sans GB",
                 "PingFang SC",
                 "Microsoft YaHei",
                 "WenQuanYi Micro Hei",
                 sans-serif;
  font-weight: 900;
  line-height: 40rpx;
}

.showPropertyBgView {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  z-index: 1000;
}

#classViewCover {
  width: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  background-color: #333;
  opacity: 0.7;
}

.popContentView {
  position: absolute;
  display: flex;
  flex-direction: column;
  top: 0;
  left: 20%;
  width: 80%;
  height: 100%;
  background-color: #fff;
}

.propertyClassView {
  width: 100%;
  height: 100%;
}

.footView {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  width: 80%;
  height: 40px;
}

.resetView {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 10px;
  width: 35%;
  height: 30px;
  line-height: 40px;
  text-align: center;
  color: #4D9FD8;
  border: 1px solid #4D9FD8;
  font-size: 15px;
  border-radius: 3px;
}

.sureView {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 10px;
  width: 55%;
  height: 30px;
  line-height: 40px;
  text-align: center;
  color: white;
  font-size: 15px;
  background-color: #4D9FD8;
  border-radius: 3px;
}

.scrollView {
  width: 100%;
  padding-bottom: 40px;
}

.priceView {
  display: flex;
  flex-direction: column;
  margin-left: 5px;
  margin-right: 5px;
  width: calc(100% - 10px);
  height: 65px;
}

.TitleText {
  margin-left: 5px;
  line-height: 20px;
  font-size: 12px;
  color: #111;
}

.priceSpaceView {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 60px;
}

.priceSpaceText {
  margin-left: 5px;
  width: 110px;
  font-size: 12px;
  color: #999;
}

.inputPriceText {
  background-color: #f0f0f0;
  font-size: 12px;
  text-align: center;
}

.priceSpace {
  width: 10px;
  height: 1px;
  background-color: #dcdcdc;
}

.popClassView {
  margin-left: 5px;
  margin-right: 5px;
}

.classesView {
  margin-top: 0px;
  width: 100%;
}

.classItem {
  display: inline-block;
  flex-direction: row;
  margin-top: 5px;
  margin-left: 5px;
  width: calc((100% - 20px)/3);
  height: 30px;
  border-radius: 5px;
}

.classItemText {
  display: inline-block;
  width: 100%;
  font-size: 12px;
  line-height: 30px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  color: #222;
}

.popPropertyView {
  display: flex;
  flex-direction: column;
  margin-top: 10px;
  width: 100%;
}

.popPropertyTitleView {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 20px;
}

.propertyOpenView {
  display: flex;
  align-items: center;
  width: 50px;
  height: 100%;
}

.openImage {
  margin-left: 17px;
  width: 15px;
  height: 8px;
}

.propertysView {
  margin-top: 0px;
  width: 100%;
  max-height: 35px;
  overflow: hidden;
}

.openBar {
  max-height: none;
  margin-top: 0px;
  width: 100%;
  overflow: hidden;
}

.show {
  display: block;
}

.hide {
  display: none;
}
